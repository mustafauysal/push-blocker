{
  "manifest_version": 2,
  "name": "Push Blocker",
  "description": "This extension blocks common push service and their pop-up",
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "http://*/",
    "https://*/"
  ]
}